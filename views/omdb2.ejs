<head>
        <title>OMDB movie</title>
        <script url="omdb.js"></script>
        <link rel="stylesheet" type="text/css" href="stylesheets/omdb2.css">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
    </head>
    <body>
        <header>
            <div id="logo">
                <a>The Entertainment Info</a>
            </div>
        </header>
        
        
        <div class="container">
        <% if(data.Response=="False"){ %>
            <div class="containerinner">
                <h1 style="margin:15%">No results!</h1>
            </div>
        <% }else { %>    

        <% data.Search.forEach(ele=>{ %>
        	<div class="containerinner" <%if(data.Search.length>1){%>style="border-bottom:1px solid white;"<%}%>>
	            <div class="image">
	            	<div style="width:100%;height:100%;"><img src="<%= ele.Poster%>" height="100%" width="100%" onerror="this.src='images/imageerr.png'" style="border-radius:5px;"></div>
	            </div>
	            <div class="info">
	            	<div class="infoinner">
	            		<h1><%= ele.Title %></h1>
	            		<h2>(<%= ele.Year %>)</h2>
	            	</div>
	            </div>
            </div>
        <%});} %>

        </div>
        
        <footer>
            <div style="width:33% ;border-right: 1px solid #151719;">
                <img src="images/hipsipic.png">
            </div>
            <div id="nameFooter" style="width:33%;border-right: 1px solid #151719;"> 
                <b><p>Made by Hipranshu</p></b>
            </div>
            <div style="width:33%">
                <button id="socialinsta"><a id="socialinstalink" href="https://www.instagram.com/hipsi.ig/?hl=en">Instagram</a></button>
                <button id="socialfb" ><a id="socialfblink" href="https://www.facebook.com/hipsi.hipranshu">Facebook</a></button>
            </div>
        </footer>
    </body>